{
  "schemaVersion": 1,
  "id": "clickthroughreborn",
  "version": "${version}",
  "name": "ClickThroughReborn",
  "description": "Click through signs and item frames to chests and other containers",
  "authors": ["NATroutter"],
  "contact": {
    "homepage": "https://NATroutter.fi",
    "sources": "https://github.com/NATroutter/ClickThroughReborn",
    "issues": "https://github.com/NATroutter/ClickThroughReborn/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/clickthroughreborn/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [ "fi.natroutter.clickthroughreborn.ClickThroughReborn" ]
  },
  "mixins": [
    "clickthroughreborn.mixins.json"
  ],
  "recommends": {
    "modmenu" : "*"
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "suggests": {
    "modmenu": ">=11.0.1"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Developer's Website": "https://natroutter.fi/",
        "Discord": "https://discord.nat.gg/"
      },
      "update_checker": false
    }
  }
}
